{
  "restartable": "rs",
  "ignore": [
    ".git/",
    "node_modules/**/node_modules",
    "operations/",
    "errors/"
  ],
  "verbose": true,
  "execMap": {
    "js": "node --harmony"
  },
  "watch": [
    "lib/*.js",
    "sections/**",
    "*.js",
    "dist/**"
  ],
  "env": {
    "DEBUG": "DEBUG=mongo,c3,api-2,analysis,sharding,staticpages,fbtrex,utils"
  },
  "ext": "js css json jade"
}
