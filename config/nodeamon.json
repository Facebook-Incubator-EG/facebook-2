{
  "watch": [
    "lib/*.js",
    "sections/**",
    "app.js",
    "dist/**"
  ],
  "ignore": [
    "bin/**",
    ".git/",
    "node_modules/**/node_modules",
    "operations/",
    "errors/",
    "sections/webscripts/*.js",
    "parsers/*",
    "mongo-scripts/*"
  ],
  "verbose": true,
  "exitcrash": false,
  "env": {
    "DEBUG": "-fbtrex,lib:*,-lib:parser,-lib:performa"
  },
  "ext": "js css json jade"
}
