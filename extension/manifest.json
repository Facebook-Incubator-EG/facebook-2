{
  "manifest_version": 2,

  "name": "Fbtrex",
  "description": "Facebook tracking exposed",
  "version": "1.0.0",

  "permissions": [
    "tabs",
    "storage",
    "https://*.facebook.com/",
    "http://localhost:8000/"
  ],
  "content_scripts": [
    {
      "matches": ["https://*.facebook.com/*"],
      "js": ["build/bundle.js"],
      "css": ["build/styles.css"]
    }
  ]

}

